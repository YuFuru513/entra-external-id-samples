@using Microsoft.AspNetCore.Authentication.OpenIdConnect

@if (!User.Identity?.IsAuthenticated ?? true)
{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="Account" asp-action="SignIn">ログイン</a>
        </li>
    </ul>
}
else
{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="Account" asp-action="SignOut">ログアウト</a>
        </li>
    </ul>
}
